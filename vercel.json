{
  "version": 2,
  "builds": [{ "src": "api/*.js", "use": "@vercel/node" }],
  "routes": [
    { "src": "/get_user", "dest": "api/get_user.js" },
    { "src": "/create_user", "dest": "api/create_user.js" },
    { "src": "/get_category", "dest": "api/get_category.js" },
    { "src": "/create_category", "dest": "api/create_category.js" },
    { "src": "/get_transaction", "dest": "api/get_transaction.js" },
    { "src": "/create_transaction", "dest": "api/create_transaction.js" },
    { "src": "/create_recurring_bill", "dest": "api/create_recurring_bill.js" },
    { "src": "/get_recurring_bill", "dest": "api/get_recurring_bill.js" }
  ]
}
